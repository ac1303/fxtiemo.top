<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name></display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <filter>
    <filter-name>login</filter-name>
    <filter-class>Filter.LoginFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>login</filter-name>
    <url-pattern>*.jsp</url-pattern>
  </filter-mapping>
  <servlet>
    <description></description>
    <display-name>uploadpicture</display-name>
    <servlet-name>uploadpicture</servlet-name>
    <servlet-class>upload.uploadpicture</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>uploadpicture</servlet-name>
    <url-pattern>/uploadpicture</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>upload_pic</display-name>
    <servlet-name>upload_pic</servlet-name>
    <servlet-class>upload.upload_pic</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>upload_pic</servlet-name>
    <url-pattern>/upload_pic</url-pattern>
  </servlet-mapping>
</web-app>